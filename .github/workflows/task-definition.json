{
    "family": "crud",
    "containerDefinitions": [
      {
        "name": "crud",
        "image": "<your_account_id>.dkr.ecr.<your_region>.amazonaws.com/crud:latest",
        "memory": 512,
        "cpu": 256,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 3000,
            "hostPort": 3000
          }
        ]
      }
    ]
  }
  